@rootpgt: PGT
  target_as=MACHPHYSADDR
  root=KPHYSADDR:0x8b5000
  pte_format=s390x
  bits=12,8,11

@machphys_kphys: LINEAR
  target_as=KPHYSADDR
  off=0x0

@kphys_machphys: LINEAR
  target_as=MACHPHYSADDR
  off=0x0

KV -> HW:
0-7fffffff: @rootpgt
80000000-ffffffffffffffff: NONE

KV -> PHYS:
0-7fffffff: @rootpgt
80000000-ffffffffffffffff: NONE

KPHYS -> DIRECT:

MACHPHYS -> KPHYS:
0-ffffffffffffffff: @machphys_kphys

KPHYS -> MACHPHYS:
0-ffffffffffffffff: @kphys_machphys
